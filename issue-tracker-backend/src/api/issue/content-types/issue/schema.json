{
  "kind": "collectionType",
  "collectionName": "issues",
  "info": {
    "singularName": "issue",
    "pluralName": "issues",
    "displayName": "Issue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subTitle": {
      "type": "string",
      "required": true
    },
    "assign_to": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high"
      ],
      "required": true,
      "default": "low"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "inProgress",
        "completed"
      ],
      "default": "new"
    },
    "completeInPercent": {
      "type": "integer",
      "default": 1
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    }
  }
}
